{
  "name": "vuex-typescript-jest",
  "version": "1.0.0",
  "description": "My Vuex TypeScript Jest Example Nuxt.js project",
  "author": "Takuya Iida",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path ../.gitignore .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "nuxt build && yarn build:copy",
    "build:copy": "rimraf ../public/* && rimraf ../functions/.nuxt/* && cpx .nuxt/**/* ../functions/.nuxt/ &&  cpx .nuxt/dist/**/* ../public/assets/ && cpx src/static/* ../public/"
  },
  "dependencies": {
    "nuxt": "^2.4.0",
    "nuxt-property-decorator": "^2.1.3",
    "vuex-class-component": "^1.6.0"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.7.1",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.8.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "vue-jest": "^3.0.4"
  }
}
